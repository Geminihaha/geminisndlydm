<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >

    <TextView
        android:id="@+id/txtv_commant_title"
        android:textStyle="bold"
        android:layout_margin="5dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/title_commant" />

    <TextView
        android:id="@+id/txtv_commant"
        android:layout_margin="5dp"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/txtv_commant_title"
        android:text="" />

    <TextView
        android:id="@+id/txtv_code_title"
        android:layout_margin="5dp"
        android:textStyle="bold"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/txtv_commant"
        android:text="@string/title_code_num" />

    <TextView
        android:id="@+id/txtv_code"
        android:layout_margin="5dp"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/txtv_code_title"
        android:text="" />

    <WebView
        android:id="@+id/webv_resulturl"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_below="@+id/txtv_code" />

</RelativeLayout>